{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxDequeueCount": 10,  
      "visibilityTimeout": "00:00:30" 
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "concurrency": {
    "dynamicConcurrencyEnabled": true,
    "snapshotPersistenceEnabled": true
  }
}